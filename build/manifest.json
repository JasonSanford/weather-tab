{
  "name": "Weather Tab",
  "description": "Show your current location and weather in new tabs.",
  "version": "0.0.1",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://api.darksky.net; object-src 'self'",
  "permissions": [
    "geolocation",
    "storage",
    "https://api.darksky.net/*"
  ],
  "manifest_version": 2
}
